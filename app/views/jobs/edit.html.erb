<h1> Edit a job </h1>

<%= simple_form_for @job do |f| %>
<%= f.input :title %>
<%= f.input :description %>
<%= f.input :wage_lower_bound, :label => "Minimum salary" %>
<%= f.input :wage_upper_bound, :label => "Maximum salary" %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>

  <%= f.submit "Submit"%>
<% end %>
