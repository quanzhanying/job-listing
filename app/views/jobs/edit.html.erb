<h1> Edit a job </h1>
  <%= simple_form_for @job do |f| %>
  <%= f.input :title %>
  <%= f.input :description%>
  <%= f.input :wage_lower_bound, :label => "最低薪资"%>
  <%= f.input :wage_upper_bound, :label => "薪资上限" %>
  <%= f.input :container_email %>
  <%= f.input :is_hiden %>
  <%= f.submit "Submit"%>
  <% end %>
