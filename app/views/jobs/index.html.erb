<div class="col-md-12">
  <div class="group">
    <%= link_to("Add a job", new_job_path, class: "btn btn-default pull-right") %>
  </div>
  <table class="table table-hover">
    <tbody>
      <% @jobs.each do |job| %>
        <tr>
          <td><%= link_to(job.title, job_path(job)) %></td>
          <td><%= job.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
