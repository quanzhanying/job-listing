<div class="col-md-12">
  <div class="group">
    <%= link_to("Add a Job", new_job_path, class: "btn btn-primary pull-right") %>
  </div>
  <table calss="table table-hover">

    <tbody>
      <% @jobs.each do |job| %>
        <tr>
          <td><%= link_to(job.title, job_path(job)) %></td>
          <td>
            <%= link_to("Edit", edit_job_path(job))%>
            <a> | </a>
            <%= link_to("Destroy", job_path(job), method: :delete, data:{confirm:"Are you sure?"} )%>
          </td>
          <td><%= job.created_at %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
