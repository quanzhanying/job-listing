<div class="col-md-12">
  <table class="table table-hover">
    <thead>
      <tr>
        <td>#</td>
        <td>职缺</td>
        <td>简介</td>
        <td>薪资</td>
        <td>联络方式</td>
      </tr>
    </thead>
    <tbody>
      <% @jobs.each do |job| %>
        <tr>
          <td>#</td>
          <td><%= link_to(job.title, job_path(job)) %></td>
          <td><%= job.description %></td>
          <td><%= job.wage_lower_bound %> ~ <%= job.wage_upper_bound %></td>
          <td><%= job.contact_email %></td>
          <td>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
