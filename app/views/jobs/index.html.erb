<div class="col-md-12">
  <div class="job">
    <%= link_to("Add a job", new_job_path, class: "btn btn-primary pull-right") %>
  </div>
  <table class="table table-hover">
    <thead>
      <tr>

        <td>Title</td>
        <td>薪资下限</td>
        <td>薪资上限</td>
        <td>Time</td>
        <td>Email</td>

      </tr>
    </thead>
    <tbody>
      <% @jobs.each do |job| %>
      <tr>
        <td>
      <%= render_job_status(job) %>
          <%= link_to(job.title, admin_job_path(job)) %>
        </td>
        <td><%= job.wage_lower_bound %></td>
        <td><%= job.wage_upper_bound %></td>
        <td><%= job.created_at %> </td>
        <td><%= job.contact_email %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
