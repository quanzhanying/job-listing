<div class="col-md-4 col-md-offset-4">
  <h2 class="text-center">发表新职位</h2>
    <%= simple_form_for @job do |f|%>
      <div class="form-group">
        <%= f.input :title,label:"职位名称",inpup_html: { class:"form-control"}%>
        <%= f.input :description,label:"职位内容",input_html: {class: "form-control"}%>
      </div>
    <%= f.submit "Submit", class: "btn btn-primary", data:{ disable_with:"Submiting..."}%>
    <% end %>
</div>
