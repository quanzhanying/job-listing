<div class="col-md-4 col-md-offset-4">
    <h2>简历投递</h2>
    <hr>
		<%= simple_form_for [@job,@resume] do |f| %>
		  <div class="form-resume">
		    <%= f.input :name, :label => "姓名"  %>
		    <%= f.input :attachment, :label => "简历" %>
		  </div>
		  <%= f.submit "Submit", class: "btn btn-primary", data: { disable_with: "Submitting..." } %>
		<% end %>


</div>
