<div class="col-md-12">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title"><%= @job.title %>/简历详情/<%= @resume.user.email %></h3>
    </div>
    <div class="panel-body">
      <p>
      <%= @resume.content %>
      </p>
      <br/>
      <%= @resume.updated_at %>
    </div>
    <div class="panel-footer"><i class="fa fa-paperclip" aria-hidden="true"></i><%= link_to "下载简历", @resume.attachment_url %></div>
  </div>
</div>
