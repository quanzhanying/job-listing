<tr>
	<td>
		<%= render_job_status(job) %>
		<%= link_to(job.title, admin_job_path(job)) %>
	</td>
	<td><%= job.updated_at %></td>
	<td><%= link_to("Edit", edit_admin_job_path(job), class: "btn btn-primary btn-sm") %> |
	<%= link_to("Delete", admin_job_path(job), class: "btn btn-primary btn-sm", method: :delete, data: {confirm: "Are you sure"}) %>
	</td>
</tr>