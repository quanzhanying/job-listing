<div class='container col-sm-8 col-sm-offset-2'>
<h1>编辑职位信息</h1>
<%= simple_form_for [:admin, @job] do |f| %>
<%= f.input :title %>
<%= f.input :description %>
<%= f.input :wage_upper_bound, :label => "薪资上限" %>
<%= f.input :wage_lower_bound, :label => "薪资下限" %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>
<%= f.submit "Submit" %>
<% end %>
</div>
