<div class="job-show-back">
<h1> Edit a job </h1>

<%= simple_form_for [:admin, @job ] do |f| %>

<%= f.input :title %>
<%= f.input :description %>
<%= f.input :wage_lower_bound, :label => "By_lower_bound" %>
<%= f.input :wage_upper_bound, :label => "By_upper_bound" %>
<%= f.input :work_place %>
<%= f.input :Company_name %>
<%= f.input :About_us %>
<%= f.input :Address %>
<%= f.input :contact_email %>
<%= f.input :is_hidden %>
<%= f.input :practice %>


<%= f.submit "Submit" %>
<% end %>
</div>
