<h1>edit</h1>

<%= simple_form_for @work do |f| %>

<%= f.input :title %>
<%= f.input :description %>
<%= f.input :wage_lower_bound, :label => "收入下限" %>
<%= f.input :wage_upper_bound, :label => "收入上限" %>
<%= f.input :contact_email, :label => "邮箱" %>
<%= f.input :is_hidden, :label => "隐藏" %>

<%= f.submit "go" %>

<% end %>
